    <script>
    // Particles.js Configuration
    particlesJS.load('particles-js', 'particles.json', function() {
        console.log('particles.js loaded');
    });

    // Function to update the Regal Timer
    function updateTimer() {
        const startDate = new Date('2022-02-01T00:00:00').getTime();
        const now = new Date().getTime();
        const timeDiff = now - startDate;

        const years = Math.floor(timeDiff / (1000 * 60 * 60 * 24 * 365));
        const months = Math.floor((timeDiff % (1000 * 60 * 60 * 24 * 365)) / (1000 * 60 * 60 * 24 * 30));
        const days = Math.floor((timeDiff % (1000 * 60 * 60 * 24 * 30)) / (1000 * 60 * 60 * 24));

        document.getElementById('regalTimer').innerHTML = `Time at Regal Security Inc.: ${years} years, ${months} months, ${days} days`;
    }

    updateTimer();
    setInterval(updateTimer, 1000 * 60 * 60 * 24); // Update daily

    // GSAP Animations
    gsap.registerPlugin(ScrollTrigger);

    gsap.utils.toArray('.section').forEach(section => {
        gsap.from(section, {
            opacity: 0,
            y: 50,
            duration: 1,
            scrollTrigger: {
                trigger: section,
                start: "top 80%",
                end: "top 50%",
                scrub: true
            }
        });
    });

    gsap.utils.toArray('.experience-item').forEach((item, index) => {
        gsap.from(item, {
            opacity: 0,
            x: index % 2 === 0 ? -50 : 50,
            duration: 1,
            scrollTrigger: {
                trigger: item,
                start: "top 80%",
                end: "top 50%",
                scrub: true
            }
        });
    });

    gsap.utils.toArray('.skill').forEach((skill, index) => {
        gsap.from(skill, {
            opacity: 0,
            scale: 0.5,
            rotation: index % 2 === 0 ? -45 : 45,
            duration: 0.5,
            scrollTrigger: {
                trigger: skill,
                start: "top 90%",
                end: "top 70%",
                scrub: true
            }
        });
    });

    gsap.utils.toArray('.cert-item').forEach((item, index) => {
        gsap.from(item, {
            opacity: 0,
            y: 30,
            duration: 0.5,
            delay: index * 0.1,
            scrollTrigger: {
                trigger: item,
                start: "top 90%",
                end: "top 70%",
                scrub: true
            }
        });
    });

    // Hover effect for certification items
    const certItems = document.querySelectorAll('.cert-item');
    certItems.forEach(item => {
        item.addEventListener('mouseenter', () => {
            gsap.to(item.querySelector('.cert-details'), {
                y: 0,
                opacity: 1,
                duration: 0.3,
                ease: 'power2.out'
            });
        });
        item.addEventListener('mouseleave', () => {
            gsap.to(item.querySelector('.cert-details'), {
                y: '100%',
                opacity: 0,
                duration: 0.3,
                ease: 'power2.in'
            });
        });
    });

    // Function to download content as PDF
    async function downloadPDF() {
        // Load jsPDF module
        const { jsPDF } = window.jspdf;

        // Create a new jsPDF instance
        const doc = new jsPDF('p', 'pt', 'a4');

        // Get the content to be converted to PDF
        const content = document.getElementById('content');

        // Remove animations and styles that may affect the PDF rendering
        document.body.classList.add('pdf-export');

        // Convert the HTML content to PDF using the html() method
        await doc.html(content, {
            callback: function (doc) {
                // Restore the original styles after PDF generation
                document.body.classList.remove('pdf-export');
                doc.save('Ajaypreet_Singh_Resume.pdf');
            },
            x: 20,
            y: 20,
            width: doc.internal.pageSize.getWidth() - 40, // page width minus margins
            windowWidth: content.scrollWidth, // Use content's width for better scaling
            html2canvas: {
                scale: 0.6, // Adjust scale for better quality
                useCORS: true,
                scrollY: 0,
            },
            margin: [20, 20, 20, 20],
            autoPaging: 'text',
        });
    }
    </script>
</body>
</html>
